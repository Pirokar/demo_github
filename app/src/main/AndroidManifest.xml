<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.sequenia.appwithchatdev">

    <permission
        android:name="com.sequenia.appwithchatdev.permission.C2D_MESSAGE"
        android:protectionLevel="signature"/>
    <permission
        android:name="com.sequenia.appwithchatdev.permission.pushserver.RECEIVE"
        android:protectionLevel="signature"/>

    <uses-permission android:name="com.sequenia.appwithchatdev.permission.C2D_MESSAGE"/>
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE"/>
    <uses-permission android:name="com.google.android.c2dm.permission.SEND"/>
    <uses-permission android:name="com.sequenia.appwithchatdev.permission.pushserver.RECEIVE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name="com.sequenia.appwithchatdev.MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <receiver
            android:name="im.threads.broadcastReceivers.MainPBReceiver"
            android:exported="false"
            android:permission="com.sequenia.appwithchatdev.permission.pushserver.RECEIVE">
            <intent-filter>
                <action android:name="com.pushserver.android.NEW_PUSH_MESSAGE_EVENT"/>
                <action android:name="com.pushserver.android.RECEIVER_CHANGED_EVENT"/>
                <action android:name="com.pushserver.android.DEVICE_ADDRESS_CHANGED_EVENT"/>
                <action android:name="com.pushserver.android.ERROR_EVENT"/>
            </intent-filter>
        </receiver>
        <meta-data
            android:name="com.pushserver.android.service"
            android:value="im.threads.services.IncomingMessagesIntentService"/>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="7dea244530b440655e927fed9654a59108f414bc"/>
        <meta-data
            android:name="com.pushserver.android.senderId"
            android:value="!STRING!1079639835466"/>
        <meta-data
            android:name="com.pushserver.android.serverUrl"
            android:value="https://pushserver.mfms.ru/brooma"/>
        <!--<meta-data-->
            <!--android:name="com.pushserver.android.serverUrl"-->
            <!--android:value="https://gatepush10.mfms.ru:18042"/>-->
        <meta-data
            android:name="com.pushserver.android.logs"
            android:value="true"/>

        <meta-data
            android:name="com.pushserver.android.location"
            android:value="true"/>
        <meta-data
            android:name="com.pushserver.android.heartbeatInterval"
            android:value="300000"/>
